1# 

SELECT COUNT(ID) AS iscritti, YEAR(enrolment_date)
FROM students
GROUP BY YEAR(enrolment_date)


2#

SELECT COUNT(ID) AS stesso_edificio, office_address
FROM teachers
GROUP BY office_address


3#

SELECT AVG(vote) AS media_voti , exam_id AS appello
FROM exam_student
GROUP BY exam_id


4# 

SELECT COUNT(ID) AS 'corsi_di_laurea', `department_id`
FROM degrees
GROUP BY `department_id`
